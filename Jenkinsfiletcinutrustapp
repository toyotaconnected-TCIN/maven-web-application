pipeline{
    agent any
    stages{
        stage('Git Checkout'){
            steps{
                git branch: 'development', credentialsId: 'GitHub_Credential', url: 'https://github.com/toyotaconnected-TCIN/maven-web-application.git'
            }
        }
        stage('mavenBuild'){
            steps{
                script{
                    sh 'mvn clean package'
                }
            }
        }
    }
}